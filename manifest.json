{
  "manifest_version": 3,
  "name": "BLS Table Extractor",
  "version": "1.0",
  "description": "Extract BLS.gov tables to CSV files",
  "permissions": [
    "activeTab",
    "scripting",
    "downloads"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
} 